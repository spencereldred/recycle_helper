<%# current_user.inspect %></br>
<%# @available.inspect %>
<div class="col-sm-offset-5 col-sm-7"><h1>All Available</h1></div>
<div class="row">
  <h4>
    <div class="col-sm-offset-2 col-sm-1">Select</div>
    <div class="col-sm-2">City / State / Zip </div>
    <div class="col-sm-1">Plastic</div>
    <div class="col-sm-1">Cans</div>
    <div class="col-sm-1">Glass</div>
    <div class="col-sm-1">Other</div>
    <div class="col-sm-2">Posted</div>
  </h4>
</div>
<div class="row">
  <h5>
    <% @available.each do |avail| %>
      <div class="row">
        <div class="col-sm-offset-2 col-sm-1">
          <form action="/redeemers/<%= avail[:id] %>" method="post">
            <input name="_method" type="hidden" value="put" />
            <input type="submit" value="Select"/>
          </form>
        </div>
        <div class=" col-sm-2">
          <% recycler = Profile.where(user_id: avail[:recycler_user_id]) %>
          <%= recycler[0][:city]%>, <%= recycler[0][:state]%> <%= recycler[0][:zipcode]%>
        </div>
        <div class="col-sm-1"><%= avail[:plastic] %></div>
        <div class="col-sm-1"><%= avail[:cans] %></div>
        <div class="col-sm-1"><%= avail[:glass] %></div>
        <div class="col-sm-1"><%= avail[:other] %></div>
        <div class="col-sm-2"><%= avail[:created_at].strftime("%b %d, %Y") %></div>
      </div>
        <% end %>
  </h5>
</div>
<div class="col-sm-offset-4 col-sm-8"><h1>Selected  by <%= @redeemer[0][:first_name]%>
  <%= @redeemer[0][:last_name]%></h1></div>
<div class="row">
  <h4>
    <div class="col-sm-offset-1 col-sm-1">Unselect</div>
    <div class="col-sm-3">Address</div>
    <div class="col-sm-1">St. / Zip</div>
    <div class="col-sm-1">Plastic</div>
    <div class="col-sm-1">Cans</div>
    <div class="col-sm-1">Glass</div>
    <div class="col-sm-1">Other</div>
    <div class="col-sm-2">Selected</div>
  </h4>
</div>
<div class="row">
  <h5>
  <% @selected.each do |select| %>
    <div class="row">
      <div class="col-sm-offset-1 col-sm-1">
          <form action="/redeemers/<%= select[:id] %>" method="post">
            <input name="_method" type="hidden" value="put" />
            <input type="submit" value="Unselect"/>
          </form>
      </div>
      <div class="col-sm-3">
        <% recycler = Profile.where(user_id: select[:recycler_user_id]) %>
        <%= recycler[0][:addr_line_1]%>, <%= recycler[0][:city]%>
          <script>

    $(function(){
          codeAddress("<%= recycler[0][:addr_line_1]%>, <%= recycler[0][:city]%> <%= recycler[0][:state]%>")
    })

  </script>
      </div>
      <div class="col-sm-1">
        <%= recycler[0][:state]%>, <%= recycler[0][:zipcode]%>
      </div>
      <div class="col-sm-1"><%= select[:plastic] %></div>
      <div class="col-sm-1"><%= select[:cans] %></div>
      <div class="col-sm-1"><%= select[:glass] %></div>
      <div class="col-sm-1"><%= select[:other] %></div>
      <div class="col-sm-2"><%= if select[:selection_date] then select[:selection_date].strftime("%b %d, %Y") end %></div>
    </div>
      <% end %>
  </h5>
</div>

